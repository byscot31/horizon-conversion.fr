---
const { ASSET_BASE = "/_assets_" } = Astro.props;
---

<!-- JS (Canvas) -->
<script src={`${ASSET_BASE}/js/plugins.min.js`}></script>
<script src={`${ASSET_BASE}/js/functions.bundle.js`}></script>

<!-- Parallax -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/skrollr/0.6.30/skrollr.min.js"></script>
<script is:inline>!SEMICOLON.Mobile.any() && skrollr.init({ forceHeight: false });</script>